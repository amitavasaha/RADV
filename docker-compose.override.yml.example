version: '3.8'

services:
  finance-agent:
    ports:
      - "9099:9099"
    environment:
      - GOOGLE_API_KEY=your_key_here
      - SERP_API_KEY=your_key_here
      - SEC_EDGAR_API_KEY=your_key_here
